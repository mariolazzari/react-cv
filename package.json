{
  "name": "@mariolazzari/react-cv",
  "private": false,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest run",
    "watch": "vitest"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.0.0",
    "@types/node": "^20.14.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react": "^4.3.1",
    "c8": "^10.1.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "jsdom": "^24.1.1",
    "react-test-renderer": "^18.3.1",
    "typescript": "^5.2.2",
    "vite": "^5.3.4",
    "vitest": "^2.0.4"
  },
  "description": "React Curriculum Vitae component",
  "main": "./dist/react-cv.umd.js",
  "module": "./dist/react-cv.es.js",
  "exports": {
    ".": {
      "import": "./dist/react-cv.es.js",
      "require": "./dist/react-cv.umd.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mariolazzari/react-cv"
  },
  "keywords": [
    "react",
    "components"
  ],
  "author": "Mario Lazzari",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mariolazzari/react-cv/issues"
  },
  "homepage": "https://github.com/mariolazzari/react-cv#readme"
}
